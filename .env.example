POSTMAN_API_KEY=your_api_key_here
# Optional: Search by Workspace Name (defaults to search all if not found)
WORKSPACE_NAME=My Workspace
# Optional: Search by Collection Name
COLLECTION_NAME=My Collection
# Optional: Direct ID override (if names are ambiguous)
COLLECTION_ID=
# Optional: Output directory for generated documentation (relative to sync.js location)
# Examples: ../doc, ../docs/api, ./my-docs (defaults to ./api-documentation)
OUTPUT_DIR=
# Optional: Output directory for API endpoint object files (relative to sync.js location)
# Examples: ../src/api/endpoints, ./api/endpoints (leave empty to skip API endpoint generation)
API_ENDPOINTS_DIR=
